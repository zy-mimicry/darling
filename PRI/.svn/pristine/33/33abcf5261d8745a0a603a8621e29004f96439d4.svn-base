        function qti_callback(record, rowindex, value, column){
            values=value.split('-');
            appsugst=values[0];
            id=values[1];
            update_cnt=values[2];
            if (values.length>3) {status=values[3];} else {status='';}
            borderstyle= (parseInt(update_cnt)<=2)?'':'border-style:solid;border-bottom-width: 0px;border-top-width: 0px;border-left-width: 2px;border-right-width: 0px;border-color:#ff0000;'
            if (gvar.showchexbox==false) {
                inputbox=" txtrlt "
            } else {
                inputbox="<input title='"+column.name+"' type=checkbox onclick='qti_update(this);' id='"+id+"' chkrlt >"
            }
            if (status=='Merged') {
                                  return "<li width='100%' height='100%' style='background:#80FF80;"+borderstyle+"' title='"+column.name+"' onclick='qti_update0(this.title);' ondblclick='qti_update1("+id+");'>"+inputbox.replace('chkrlt',' checked').replace('txtrlt','Yes')+"</li>";
            } else {
              if (appsugst=='YN') return "<li width='100%' height='100%' style='background:#FFFFFF;"+borderstyle+"' title='"+column.name+"' onclick='qti_update0(this.title);' ondblclick='qti_update1("+id+");'>"+inputbox.replace('chkrlt',' checked').replace('txtrlt','Yes')+"</li>";
              if (appsugst=='YY') return "<li width='100%' height='100%' style='background:yellow;"+borderstyle+"' title='"+column.name+"' onclick='qti_update0(this.title);' ondblclick='qti_update1("+id+");'>"+inputbox.replace('chkrlt',' checked').replace('txtrlt','<font color=red>Yes</font>')+"</li>";
              if (appsugst=='NY') return "<li width='100%' height='100%' style='background:yellow;"+borderstyle+"' title='"+column.name+"' onclick='qti_update0(this.title);' ondblclick='qti_update1("+id+");'>"+inputbox.replace('chkrlt',' ').replace('txtrlt','<font color=red>No</font>')+"</li>";
              if (appsugst=='NN') return "<li width='100%' height='100%' style='"+borderstyle+"' title='"+column.name+"' onclick='qti_update0(this.title);' ondblclick='qti_update1("+id+");'>"+inputbox.replace('chkrlt',' ').replace('txtrlt','No')+"</li>";
            }
        }
